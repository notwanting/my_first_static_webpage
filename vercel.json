{
    "version": 2,
    "builds": [
        {
            "src": "bilibili.html",
            "use": "@vercel/static"
        },
        {
          "src": "public/**",
          "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/",
            "dest": "/bilibili.html"
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/$1"
        }
    ]
}
